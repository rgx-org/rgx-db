	[10006] = {
		Name = TUT_COMBAT_ATTACKING,
		Title = "Information ≫ Combat Mechanics ≫ Attacking",
		Parent = TUT_COMBAT,
		Previous = TUT_NONE,
		Next = TUT_COMBAT_ATKTYPES,
		Nav = 1,
		Search = 1,
		Page = {
			"- Click on something with Mouse 1 to attack it.\n- Hold Mouse 1 over a target to continuously auto attack it.\n- Hold CTRL while clicking on something to continuously attack it without holding Mouse 1.\n  - Note that CTRL click auto attacking may delay targetted skill usage. \n  - A common technique around this to move cancel the auto attack, then use a skill.\n  - Another technique is to use a self-cast skill (e.g. First Aid) before using your skill.\n- Many target skills that normally only work on allies or enemies will cast on different target types if you hold Shift while selecting your target.\n\nTargeting tips:\n- Your current targeted unit is the one whose name you can see.\n- A unit's targeting box is generally equal to its sprite area.\n- Changing your camera angle or zooming may help with targeting.\n- For more tricky targeting cases, try targeting at the corners of the unit's sprite. Often a target's attack animation moves their hitboxes, allowing for timing windows of precise targeting even if many units overlap.\n- Knockback AoE skills are generally effective at breaking clustered targets apart.\n\nNotes:\n- On most clients, you can enable CTRL Click behavior by default with the /noctrl or /nc commands.\n- On most clients. you can enable Shift Click behavior by default with the /noshift or /ns commands.\n\n<URL>[↗]<INFO>https://runebreak.net/manual/information/combat_mechanics/combat_attacking</INFO></URL>",
		},
		Image = "유저인터페이스\\tipbox\\img_noimage",
		Imgcoord = { Position = 5, Size = 100 },
	},
	[10007] = {
		Name = TUT_COMBAT_ATKTYPES,
		Title = "Information ≫ Combat Mechanics ≫ Attack types",
		Parent = TUT_COMBAT,
		Previous = TUT_COMBAT_ATTACKING,
		Next = TUT_COMBAT_RANGE,
		Nav = 1,
		Search = 1,
		Page = {
			"- There are three categories of attacks: Weapon, Magic, and Misc.\n- Monster auto attacks are always considered Weapon attacks.\n- Skills can be of any type. Refer to the specific skill information to view their type.\n\n<URL>[↗]<INFO>https://runebreak.net/manual/information/combat_mechanics/combat_atktypes</INFO></URL>",
		},
		Image = "유저인터페이스\\tipbox\\img_noimage",
		Imgcoord = { Position = 5, Size = 100 },
	},
	[10008] = {
		Name = TUT_COMBAT_RANGE,
		Title = "Information ≫ Combat Mechanics ≫ Range",
		Parent = TUT_COMBAT,
		Previous = TUT_COMBAT_ATKTYPES,
		Next = TUT_COMBAT_CASTING,
		Nav = 1,
		Search = 1,
		Page = {
			"- Weapon skills have two ranges: Close range (`Melee attacks`) & Long range (`Ranged attacks`).\n- A weapon attack is considered a ranged attack when the attacker is greater than 3 cells away from their target.\n- If your mainhand weapon has greater than 3 cells of range by default (i.e. a Bow), most attacks are considered ranged even if you are close to an opponent.\n- Some weapon skills have a hard defined range. Others take the range of your main hand weapon.\n- NOTE TO SELF: some skills use mob distance, note these\n\nRange tips:\n- The Pneuma skill blocks all ranged attacks.\n- The Safety Wall skill blocks melee attacks.\n- After a weapon skill begins casting, the target can move out of range. If the target is out of range when the skill completes casting, the skill will cancel. \n  - Skills gain +2 cells of range in this scenario, so be sure to move a little farther than you normally would.\n- You cannot walk out of range of Magic or Misc skills, only weapon skills.\n  - Monsters play a subtle effect when they begin casting a Magic or Misc skill. Someday I'll put a picture of this to the left, but it's small particles floating upwards.\n\nNotes:\n- For the purpose of item effects, Magic & Misc type attacks do not have a weapon range. When an item references Ranged attacks, it implies Weapon attacks only.\n- When you are out of range of a target, you will generally move into range automatically.\n\n<URL>[↗]<INFO>https://runebreak.net/manual/information/combat_mechanics/combat_range</INFO></URL>",
		},
		Image = "유저인터페이스\\tipbox\\img_noimage",
		Imgcoord = { Position = 5, Size = 100 },
	},
	[10009] = {
		Name = TUT_COMBAT_CASTING,
		Title = "Information ≫ Combat Mechanics ≫ Casting",
		Parent = TUT_COMBAT,
		Previous = TUT_COMBAT_RANGE,
		Next = TUT_COMBAT_SUPERARMOR,
		Nav = 1,
		Search = 1,
		Page = {
			"- There are three categories of casts: Interruptible, Super Armor, and Uninterruptible.\n- Interruptible skill casts will cancel when the caster receives any active damage.\n- Super Armor prevents interruptible skill casts from canceling for as long as the caster has it. See the article:\n<TIPBOX>Super Armor<INFO>" .. TUT_COMBAT_SUPERARMOR .. "</INFO></TIPBOX>\n- Uninterruptible skills cannot be cancelled by any means. Generally only monster skills are uninterruptible. The monster will play a large effect and have a blurred sprite while casting uninterruptible skills. I'll put a picture someday!\n\nNotes:\n- You cannot move while casting by default.\n- You cannot cancel casting by default.\n- When a monster is performing an uninterruptible cast, it gains immunity to status effects that prevent it from casting (e.g. Stun, Freeze, Silence, Stone Curse, Sleep, etc).\n  - Minibosses, FoEs, and Bosses do not gain immunity to Silence in this state, as it does not prevent them from casting.\n\n<URL>[↗]<INFO>https://runebreak.net/manual/information/combat_mechanics/combat_casting</INFO></URL>",
		},
		Image = "유저인터페이스\\tipbox\\img_noimage",
		Imgcoord = { Position = 5, Size = 100 },
	},
	[10010] = {
		Name = TUT_COMBAT_SUPERARMOR,
		Title = "Information ≫ Combat Mechanics ≫ Super Armor",
		Parent = TUT_COMBAT,
		Previous = TUT_COMBAT_CASTING,
		Next = TUT_COMBAT_HEALING,
		Nav = 1,
		Search = 1,
		Page = {
			"- Super armor comes as an inherent effect on some skills, or as a bonus from gear, buffs, or items.\n- When casting with super armor, players briefly flash white. \n  - Monsters flash white and gain a larger effect. I'll put a picture someday.\n- Super armor activates on skill cast, and protects the cast from being interrupted for as long as super armor is active. It ends when the skill finishes casting or super armor is broken.\n- Breaks after a damage threshold is reached. When it breaks, the cast is interrupted.\n- The damage threshold is expressed as a percentage of max HP. Each skill or modifier tends to have its own threshold.\n  - For example, if a bonus says `Storm Gust gains +20% Super Armor`, it implies that Storm Gust casts will not cancel until you've taken at least 20% of your max HP as damage while casting it.\n- When super armor breaks, it enters a cooldown period. No casts will gain super armor during this period.\n- The base cooldown timer is 10 seconds, reducable by VIT.\n- Every 1 VIT reduces your Super Armor cooldown timer by 1%.\n  - At 100 VIT your super armor can still be broken, but it resets instantly.\n\nNotes:\n- Stunning a unit casting with super armor will immediately break it.\n- Super Armor reduces your damage motion to 0, similar to Endure.\n- If a skill has `Full Super Armor`, that means the super armor value is equal to your HP value. Some effects can still break Super Armor in this scenario (e.g. Stun, Spell Breaker Lv4+), but the damage threshold is effectively infinite.\n- If you are currently mid-cast and super armor regenerates, the cast gains super armor.\n- A cast's remaining super armor percentage can be tracked next to their name.\n- Monster skills can have super armor arbitrarily.\n- Monsters of class Elite, Miniboss, FoE, and MVP have a base cooldown timer of 3 seconds.\n\n<URL>[↗]<INFO>https://runebreak.net/manual/information/combat_mechanics/combat_superarmor</INFO></URL>",
		},
		Image = "유저인터페이스\\tipbox\\img_noimage",
		Imgcoord = { Position = 5, Size = 100 },
	},
	[10011] = {
		Name = TUT_COMBAT_HEALING,
		Title = ".. ≫ Combat Mechanics ≫ Healing & Restoration",
		Parent = TUT_COMBAT,
		Previous = TUT_COMBAT_SUPERARMOR,
		Next = TUT_COMBAT_ELEMENTS,
		Nav = 1,
		Search = 1,
		Page = {
			"- Heal Power increases healing output.\n- Restoration increases incoming healing (multiplier on incoming heal value). It is effectively the same as Heal Power but an additional multiplier that the receiver itemizes.\n\n<URL>[↗]<INFO>https://runebreak.net/manual/information/combat_mechanics/combat_healing</INFO></URL>",
		},
		Image = "유저인터페이스\\tipbox\\img_noimage",
		Imgcoord = { Position = 5, Size = 100 },
	},
	[10012] = {
		Name = TUT_COMBAT_ELEMENTS,
		Title = ".. ≫ Combat Mechanics ≫ Elements & Properties",
		Parent = TUT_COMBAT,
		Previous = TUT_COMBAT_HEALING,
		Next = TUT_COMBAT_TAXONOMY,
		Nav = 1,
		Search = 1,
		Page = {
			"- An `element` refers to the element of an attack.\n- A `property` refers to the elemental property of a unit.\n- The elemental effectiveness chart for properties and elements are the same. The difference is simply in whether the element applies to an offensive action (elemental attack) or unit property (property).\n- For example, if a bonus gives you Fire Resist +25%, it applies to Fire elemental attacks (e.g. Fire Bolt, Meteor Storm). It does not apply to Fire property monsters.\n- If a bonus gives you -25% Damage from Fire Property, it applies to any attack from a Fire property unit (e.g. Drops, Agni). This effect works even if the enemy uses a non-fire attack against you.\n\n<URL>[↗]<INFO>https://runebreak.net/manual/information/combat_mechanics/combat_elements</INFO></URL>",
		},
		Image = "유저인터페이스\\tipbox\\img_noimage",
		Imgcoord = { Position = 5, Size = 100 },
	},
