			"≫ <TIPBOX>Attacking<INFO>" .. TUT_COMBAT_ATTACKING .. "</INFO></TIPBOX>\n≫ <TIPBOX>Attack types<INFO>" .. TUT_COMBAT_ATKTYPES .. "</INFO></TIPBOX>\n≫ <TIPBOX>Range<INFO>" .. TUT_COMBAT_RANGE .. "</INFO></TIPBOX>\n≫ <TIPBOX>Casting<INFO>" .. TUT_COMBAT_CASTING .. "</INFO></TIPBOX>\n≫ <TIPBOX>Super Armor<INFO>" .. TUT_COMBAT_SUPERARMOR .. "</INFO></TIPBOX>\n≫ <TIPBOX>Healing & Restoration<INFO>" .. TUT_COMBAT_HEALING .. "</INFO></TIPBOX>\n≫ <TIPBOX>Elements & Properties<INFO>" .. TUT_COMBAT_ELEMENTS .. "</INFO></TIPBOX>\n≫ <TIPBOX>Delays<INFO>" .. TUT_COMBAT_DELAYS .. "</INFO></TIPBOX>\n≫≫ <TIPBOX>After Cast Delay<INFO>" .. TUT_DELAY_ACD .. "</INFO></TIPBOX>\n≫≫ <TIPBOX>Attack Motion<INFO>" .. TUT_DELAY_AMOTION .. "</INFO></TIPBOX>\n≫≫ <TIPBOX>Damage Motion<INFO>" .. TUT_DELAY_DMOTION .. "</INFO></TIPBOX>\n≫≫ <TIPBOX>Cooldown<INFO>" .. TUT_DELAY_COOLDOWN .. "</INFO></TIPBOX>\n≫ <TIPBOX>Gear swapping<INFO>" .. TUT_COMBAT_SWAP .. "</INFO></TIPBOX>\n≫ <TIPBOX>Damage formula<INFO>" .. TUT_COMBAT_FORMULA .. "</INFO></TIPBOX>\n≫ <TIPBOX>Active damage & Passive damage<INFO>" .. TUT_COMBAT_ACTIVE .. "</INFO></TIPBOX>\n≫ <TIPBOX>Notable skills<INFO>" .. TUT_COMBAT_SKILLS .. "</INFO></TIPBOX>\n\n<URL>[↗]<INFO>https://runebreak.net/manual/information/combat_mechanics</INFO></URL>",
