	[10015] = {
		Name = TUT_DELAY_ACD,
		Title = ".. ≫ Delays ≫ After Cast Delay",
		Parent = TUT_COMBAT_DELAYS,
		Previous = TUT_NONE,
		Next = TUT_DELAY_AMOTION,
		Nav = 1,
		Search = 1,
		Page = {
			"- After Cast Delay (ACD, sometimes just `cast delay`) is a global cooldown on skill usage.\n- While ACD is in effect, you cannot cast any skills, however you can still perform normal attacks, use items, or swap gear.\n- ACD is represented by all ACD effected skills gaining a cooldown animation on your hotkey bar.\n\n<URL>[↗]<INFO>https://runebreak.net/manual/information/combat_mechanics/delays/delay_acd</INFO></URL>",
		},
		Image = "유저인터페이스\\tipbox\\img_noimage",
		Imgcoord = { Position = 5, Size = 100 },
	},
	[10016] = {
		Name = TUT_DELAY_AMOTION,
		Title = ".. ≫ Combat Mechanics ≫ Delays ≫ Attack Motion",
		Parent = TUT_COMBAT_DELAYS,
		Previous = TUT_DELAY_ACD,
		Next = TUT_DELAY_DMOTION,
		Nav = 1,
		Search = 1,
		Page = {
			"- Attack Motion (amotion, also referred to as `ASPD delay`) is an ASPD based animation delay on most actions.\n- Raising your ASPD lowers your attack motion delay.\n- When you cannot use skills and you are not in an after cast delay, it is likely that you are in attack motion delay.\n- Most weapons skills an even many magic cast animations are affected by attack motion.\n- `Attack motion`, as a term, refers to the motion a character sprite does during an attack or cast. When you raise your ASPD, you are effectively increasing the cancel window of most animations.\n\n<URL>[↗]<INFO>https://runebreak.net/manual/information/combat_mechanics/delays/delay_amotion</INFO></URL>",
		},
		Image = "유저인터페이스\\tipbox\\img_noimage",
		Imgcoord = { Position = 5, Size = 100 },
	},
	[10017] = {
		Name = TUT_DELAY_DMOTION,
		Title = ".. ≫ Combat Mechanics ≫ Delays ≫ Damage Motion",
		Parent = TUT_COMBAT_DELAYS,
		Previous = TUT_DELAY_AMOTION,
		Next = TUT_DELAY_COOLDOWN,
		Nav = 1,
		Search = 1,
		Page = {
			"- Damage Motion (dmotion, also referred to as `hit stun` or `flinching`) is the amount of time a unit staggers after receiving damage.\n- Lowering your damage motion means you spend less time flinching from damage.\n- While your damage motion is active, you cannot move or use skills.\n- `Damage motion`, as a term, refers to the motion a character sprite does during after taking damage. When you reduce your dmotion, you are lowering that animation time.\n- The `Endure` skill effectively sets your damage motion to 0.\n\n<URL>[↗]<INFO>https://runebreak.net/manual/information/combat_mechanics/delays/delay_dmotion</INFO></URL>",
		},
		Image = "유저인터페이스\\tipbox\\img_noimage",
		Imgcoord = { Position = 5, Size = 100 },
	},
	[10018] = {
		Name = TUT_DELAY_COOLDOWN,
		Title = ".. ≫ Combat Mechanics ≫ Delays ≫ Cooldowns",
		Parent = TUT_COMBAT_DELAYS,
		Previous = TUT_DELAY_DMOTION,
		Next = TUT_NONE,
		Nav = 1,
		Search = 1,
		Page = {
			"- A cooldown (or just `CD`) is an individual hard delay on skills.\n- Most skills do not have individual cooldowns, but there are notable exceptions such as `Endure`.\n- Bonuses can modify, add, or remove a skill's cooldown timer.\n- When a skill is in cooldown, it is not available for usage, but all other skills that are not on cooldown can still be used.\n\n<URL>[↗]<INFO>https://runebreak.net/manual/information/combat_mechanics/delays/delay_cooldown</INFO></URL>",
		},
		Image = "유저인터페이스\\tipbox\\img_noimage",
		Imgcoord = { Position = 5, Size = 100 },
	},
	[10019] = {
		Name = TUT_COMBAT_SWAP,
		Title = "Information ≫ Combat Mechanics ≫ Gear swapping",
		Parent = TUT_COMBAT,
		Previous = TUT_COMBAT_DELAYS,
		Next = TUT_COMBAT_SKILLS,
		Nav = 1,
		Search = 1,
		Page = {
			"- Press 'Swap Equipment' in the equipment window to setup a second load out.\n- Press 'Space' (default) to swap between loadouts\n- Swapping between loadouts has an 8 second cooldown.\n- There is no cooldown on individual gear swaps.\n\n<URL>[↗]<INFO>https://runebreak.net/manual/information/combat_mechanics/combat_swap</INFO></URL>",
		},
		Image = "유저인터페이스\\tipbox\\img_noimage",
